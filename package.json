    "easysoap": "^2.2.0",
    "exceljs": "^3.7.0",
    "fuse.js": "^3.4.5",
    "generate-password": "^1.4.1",
    "helmet": "^3.22.0",
    "js-sha256": "^0.9.0",
    "jsonwebtoken": "^8.4.0",
    "mailgun-js": "^0.22.0",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.3.16",
    "mongoose-fuzzy-searching": "^1.1.8",
    "morgan": "^1.9.1",
