    "@nestjs/microservices": "^6.10.13",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.10.13",
    "@nestjs/swagger": "^4.1.2",
    "@sentry/node": "^5.15.4",
    "@types/cors": "^2.8.4",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mongoose": "^5.3.4",
    "aes256": "^1.0.4",
